    "autoprefixer": "^10.4.0",
    "ava": "^3.15.0",
    "esbuild": "^0.14.0",
    "esbuild-plugin-postcss2": "^0.1.1",
    "postcss": "^8.4.4",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2",
    "xo": "^0.47.0"
  }
