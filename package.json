    "ava": "^3.15.0",
    "esbuild": "^0.14.0",
    "esbuild-plugin-postcss2": "^0.1.1",
    "esbuild-svelte": "^0.6.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.4",
